	<!DOCTYPE html>
	<html>
	<head>
	  <title>Tasksbox</title>
	  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
	  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
	  <%= javascript_include_tag "https://www.google.com/jsapi" %>

	  <script src="//cdn.datatables.net/1.10.12/js/jquery.dataTables.min.js"></script>
	  <link href="//cdn.datatables.net/1.10.12/css/jquery.dataTables.min.css" rel="stylesheet">

	<!-- Latest compiled and minified CSS -->
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

	<!-- Optional theme -->
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css" integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp" crossorigin="anonymous">

	
	<!-- Latest compiled and minified JavaScript -->
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>


	  <%= csrf_meta_tags %>
	</head>
	<body>

	<div class="container">


	<%if user_signed_in? %>

	<nav class="navbar navbar-default">
	  <div class="container-fluid">
	    <!-- Brand and toggle get grouped for better mobile display -->
	    <div class="navbar-header">
	      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
	        <span class="sr-only">Toggle navigation</span>
	        <span class="icon-bar"></span>
	        <span class="icon-bar"></span>
	        <span class="icon-bar"></span>
	      </button>
	      <a class="navbar-brand" href="#">Taskbox</a>
	    </div>

	    <!-- Collect the nav links, forms, and other content for toggling -->
	    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
	      <ul class="nav navbar-nav">
	        <li>
	        
	        <%=link_to clients_path do %>
	        	Clients
	        	<span class="badge"><%=current_user.is_admin? ? Client.all.size : current_user.clients.size %></span>
	        <%end %>
	        </li>



	        <li class="Projects">
	          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Projects 

	          <span class="badge"><%= current_user.projects.size %></span>

	          <span class="caret"></span></a>
	          <ul class="dropdown-menu">

	            <li><%=link_to projects_path do %>
	            <span class="glyphicon glyphicon-th-list"> </span> 
	            List All
	            <% end %>
	            </li>

	            <li><%=link_to new_project_path do %>
	            <span class ="glyphicon glyphicon-plus"></span>
	            New Projects
	            <% end %>
	            </li>

	          </ul>

	        </li>

	        <li>
	        
	        <%=link_to announcements_path do %>
	        	Announcements
	        	<span class="badge"></span>
	        <%end %>
	        </li>

	      </ul>
	      <form class="navbar-form navbar-left">
	        <div class="form-group">
	          <input type="text" class="form-control" placeholder="Search">
	        </div>
	        <button type="submit" class="btn btn-default">Submit</button>
	      </form>
	      <ul class="nav navbar-nav navbar-right">

	        <li><a href="#">Link</a></li>
	        <li class="dropdown">
	          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Dropdown <span class="caret"></span></a>
	          <ul class="dropdown-menu">
	            <li><%=link_to "Categories",categories_path %></li>
	          
	          </ul>
	        </li>

	        <li class="dropdown">
	          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Dropdown <span class="caret"></span></a>
	          <ul class="dropdown-menu">
	            <li><%=link_to "Account Settings",edit_user_registration_path%></li>
	            <li><%=link_to "Logout",destroy_user_session_path,method: :delete %></li>
	          
	          </ul>
	        </li>
	      </ul>
	    </div><!-- /.navbar-collapse -->
	  </div><!-- /.container-fluid -->
	</nav>

	<% if notice %>
		<div class="alert alert-success" role="alert"><%= notice %></div>
	<% end %>

	<% if alert %>
		<div class="alert alert-danger" role="alert"><%= alert %></div>
	<% end %>


	Howdy <%=current_user.email %>,

	<%= link_to "Clients",clients_path%>
	<%= link_to "Projects",projects_path%>
	<%=link_to "Categories",categories_path %>

	<%= link_to "Edit Account", edit_user_registration_path %>
	<%=link_to "Logout",destroy_user_session_path, method: :delete %>

	<%else %>

	<%= link_to "User",new_user_session_path %>
	<%= link_to "Sign Up",new_user_registration_path %>

	<%end %>



	<%= notice %>
	<%= alert %>

	<%= yield %>

	</body>
	</html>
